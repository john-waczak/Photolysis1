@article{ISI:000237167100029,
	title        = {{Using probability distribution functions for satellite validation}},
	author       = {Lary, D. J. and Lait, L.},
	year         = 2006,
	month        = {may},
	journal      = {IEEE Transactions on Geoscience and Remote Sensing},
