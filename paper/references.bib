@article{ISI:000253716000001,
	title        = {{QBO and annual cycle variations in tropical lower stratosphere trace gases from HALOE and Aura MLS observations}},
	author       = {Schoeberl, Mark R. and Douglass, A. R. and Newman, P. A and Lait, L. and Lary, D. J. and Livesey, N. J and Froidevaux, L. and Lambert, A. and Read, W. G and Filipiak, M. J. and Others and Waters, J. W. and Livesey, N. J and Froidevaux, L. and Lambert, A. and Read, W. G and Filipiak, M. J. and Pumphrey, H. C.},
