@article{Espedal:2001a,
	title        = {Oil Spill and Its Look-Alikes in ERS SAR Imagery},
	author       = {Espedal, H. A.},
	year         = 2001,
	journal      = {Earth Observation and Remote Sensing},
	volume       = 16,
	number       = 5,
	pages        = {813--825},
	note         = {Times Cited: 4},
	abstract     = {A study of oil spills and their look-alikes in ERS SAR imagery is performed. The slicks are classified according to characteristic image expressions, backscatter profiles and gradients, geographical occurrence and weather limitations. The radar backscatter cross section and the radar contrast or damping ratio, defined as the ratio of the backscattered radar power from slick-free and slick-covered surfaces, are calculated for the different types of slick surfaces. A conceptual model for distinguishing oil spill from other slick expressions is proposed. The model performs best when auxiliary data are available.}
}
