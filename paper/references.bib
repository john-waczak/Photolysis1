@article{Nunziata:2008,
	title        = {On the Mueller Scattering Matrix for Sar Sea Oil Slick Observation},
	author       = {Nunziata, F. and Gambardella, A. and Migliaccio, M.},
	year         = 2008,
	journal      = {Ieee Geoscience and Remote Sensing Letters},
	volume       = 5,
	number       = 4,
	pages        = {691--695},
	note         = {ISI Document Delivery No.: 373FO Times Cited: 10 Cited Reference Count: 15 Nunziata, Ferdinando Gambardella, Attilio Migliaccio, Maurizio IEEE-INST ELECTRICAL ELECTRONICS ENGINEERS INC PISCATAWAY},
	abstract     = {In this letter, a fully polarimetric electromagnetic model for sea surface Mueller matrix is exploited to characterize the scattering from oil and biogenic slicks, under low-to-moderate wind conditions. The model predicts a completely different scattering mechanism for oil-covered and oil-free sea surface, while biogenic slicks are indistinguishable from sea surface in terms of polarimetric scattering. Following this theoretical rationale, a simple and very effective filtering technique is proposed for synthetic aperture radar (SAR) sea oil slick observation. Experiments, accomplished over C-band multilook complex SIR-C/X-SAR data, show the effectiveness of the proposed model and the capabilities of the filter to both observe oil slicks and distinguish them from biogenic look-alikes.},
	keywords     = {Mueller matrix oil slicks scattering synthetic aperture radar (SAR) RADAR POLARIMETRY CLASSIFICATION OCEAN}
}
@article{Nunziata:2009,
	title        = {The Two-Scale Bpm Scattering Model for Sea Biogenic Slicks Contrast},
	author       = {Nunziata, F. and Sobieski, P. and Migliaccio, M.},
	year         = 2009,
	journal      = {Ieee Transactions on Geoscience and Remote Sensing},
	volume       = 47,
	number       = 7,
	pages        = {1949--1956},
	note         = {ISI Document Delivery No.: 463MV Times Cited: 6 Cited Reference Count: 32 Nunziata, Ferdinando Sobieski, Piotr Migliaccio, Maurizio IEEE-INST ELECTRICAL ELECTRONICS ENGINEERS INC PISCATAWAY},
	abstract     = {Sea oil slick observation by means of synthetic aperture radar is still a scientific and operational challenge. In this paper, the sea surface scattering with and without biogenic slicks is analyzed by using the two-scale Boundary Perturbation Method scattering model. The surface slick is supposed to modify both the full-range sea surface spectrum and the slope probability density function by means of the Marangoni damping and by a reduced friction velocity. In this paper, the full-range Universite Catholique de Louvain sea surface spectrum is considered. A new contrast model, which overcomes the drawbacks of the contrast model based on the untilted Small Perturbation Method scattering model, is presented and illustrated in some L-and C-band biogenic slick cases.},
	keywords     = {Sea surface electromagnetic scattering synthetic aperture radar (SAR) ROUGH-SURFACE SCATTERING OIL-SPILL DETECTION PERTURBATION METHOD SAR IMAGES FILMS WAVES OCEAN EXPANSION}
}
@article{Otremba:2001,
	title        = {Modelling of the Optical Contrast of an Oil Film on a Sea Surface},
	author       = {Otremba, Z. and Piskozub, J.},
	year         = 2001,
	journal      = {Optics Express},
	volume       = 9,
	number       = 8,
	pages        = {411--416},
	note         = {Otremba, Z Piskozub, J},
	abstract     = {The water-leaving radiance field above a sea surface polluted by an oil film has been modelled using a Monte Carlo radiative transfer technique with large numbers of photons incident at a selected zenith angle. The calculated radiance was recorded for each of the 240 sectors of equal solid angle the upper hemisphere had been divided into. The results are presented in the form of a bi-directional reflectance distribution function (BRDF) and as a contrast function parameterised by observation angle for various angles of incident sunlight and for various states of the sea surface roughness. The conditions for observing maximal and minimal contrast are described. (C) 2001 Optical Society of America.}
}
@article{Otremba:2003,
	title        = {Modeling the Remotely Sensed Optical Contrast Caused by Oil Suspended in the Sea Water Column},
	author       = {Otremba, Z. and Piskozub, J.},
	year         = 2003,
	journal      = {Optics Express},
	volume       = 11,
	number       = 1,
	pages        = {2--6},
	note         = {Otremba, Z Piskozub, J},
	abstract     = {The reflectance of sea areas polluted by an oil-in-water emulsion was modeled using the radiance transfer Monte Carlo code. Example results of the contrast function parameterized by the observation angle for various angles of incident sunlight, various sea surface roughness states and two optically different types of seawaters are presented. (C) 2003 Optical Society of America.}
}
@article{Otremba:2004a,
	title        = {Modelling the Bidirectional Reflectance Distribution Function (Brdf) of Seawater Polluted by an Oil Film},
	author       = {Otremba, Z. and Piskozub, J.},
	year         = 2004,
	journal      = {Optics Express},
	volume       = 12,
	number       = 8,
	pages        = {1671--1676},
	note         = {Otremba, Z Piskozub, J},
	abstract     = {The Bi-directional Reflectance Distribution Function (BRDF) of both clean seawaters and those polluted with oil film was determined using the Monte Carlo radiative transfer technique in which the spectrum of complex refractive index of Romashkino crude oil and the optical properties of case II water for chosen wavelengths was considered. The BRDF values were recorded for 1836 solid angular sectors of throughout the upper hemisphere. The visibility of areas polluted with oil observed from various directions and for various wavelengths is discussed. (C) 2004 Optical Society of America.}
}
@article{Otremba:2004b,
	title        = {Phase Functions of Oil-in-Water Emulsions},
	author       = {Otremba, Z. and Piskozub, J.},
	year         = 2004,
	journal      = {Optica Applicata},
	volume       = 34,
	number       = 1,
	pages        = {93--99},
	note         = {Otremba, Z Piskozub, J},
	abstract     = {This paper presents the modelling of optical phase function (PF) of water polluted by dispersed oil. The shapes of PFs for various oil droplet size distributions and for two optically extremely different oil types are shown for various wavelengths from 350 to 750 nm. It is proved that changes of optical properties of oil (the complex refractive index) play minor role in PF shaping towards the impact of wavelength and size distribution. Water with oil emulsion has a PF significantly different from that of natural ocean water or harbour turbid water.}
}
@article{Otremba:2009,
	title        = {Polarized Phase Functions in Oil-in-Water Emulsion},
	author       = {Otremba, Z. and Piskozub, J.},
	year         = 2009,
	journal      = {Optica Applicata},
	volume       = 39,
	number       = 1,
	pages        = {129--133},
	note         = {Otremba, Zbigniew Piskozub, Jacek},
	abstract     = {Results of modeling of polarized phase functions(PPFs) in water polluted by oil-in-water emulsion arc presented. The shapes of PPFs for various Oil droplets size distributions and for two optically different oil types,ire shown for various wavelengths in the visible region. It is revealed that PPFs for two perpendicular planes are different for angles greater than 50 degrees (with even 2-fold difference close to 90 degrees). Shapes of PPFs depend on the type of oil and on wavelengths oil droplets size distribution plays a minor role only.}
}
@article{Otremba:2003,
	title        = {Modelling the Reflectance of Sea Areas Polluted with Oil Emulsion},
	author       = {Otremba, Z. and Piskozub, J. and Krol, T.},
	year         = 2003,
	journal      = {Fresenius Environmental Bulletin},
	volume       = 12,
	number       = 9,
	pages        = {1109--1113},
	note         = {Otremba, Z Piskozub, J Krol, T},
	abstract     = {This paper presents the modeling of the radiance reflectance in sea areas polluted with dispersed oil. Two modeling stages are described. The first is the application of the Mie theory to determine the inherent optical properties of oil-in-seawater emulsions. The second is the Monte Carlo code to simulate radiance transfer to determine reflectance with various oil concentrations under various light and wind conditions. The resultant reflectance values contribute to the validation of remote sensing algorithms and the detection of oil spills. The optical visibility of polluted sea areas is discussed.}
}
@article{Peacock:2010,
