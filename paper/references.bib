@article{ISI:000253716000001,
	title        = {{QBO and annual cycle variations in tropical lower stratosphere trace gases from HALOE and Aura MLS observations}},
	author       = {Schoeberl, M. R. and Douglass, A. R. and Newman, P. A and Lait, L. and Lary, D. J. and Waters, J. W. and Livesey, N. and Froidevaux, L. and Lambert, A. and Read, W. and Filipiak, M. J. and Pumphrey, H. C.},
