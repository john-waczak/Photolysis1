	title        = {{Ambient air pollution and its influence on human health and welfare: an overview}},
	author       = {Almetwally, Alsaid Ahmed and Bin-Jumah, May and Allam, Ahmed A.},
	year         = 2020,
	journal      = {Environmental Science and Pollution Research},
